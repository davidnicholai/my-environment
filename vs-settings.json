{
    "files.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/lib": true
    },
    "editor.fontSize": 12,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "all",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "window.zoomLevel": 0,
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": ["<leader>","f"],
            "after": ["<C-f>"]
        },
        {
            "before": ["<leader>","b"],
            "after": ["<C-b>"]
        },
        {
            "before": ["<leader>","p"],
            "after": ["\"", "0", "p"]
        },
        {
            "before": ["<leader>","h"],
            "after": [],
            "commands": [
                {
                    "command": ":noh"
                }
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys":{
        "<C-a>": false,
        "<C-f>": false,
        "<C-w>": false,
        "<C-v>": false,
        "<C-c>": false
    }
}
